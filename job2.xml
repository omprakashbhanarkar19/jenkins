<?xml version='1.0' encoding='UTF-8'?>
<flow-definition plugin="workflow-job@2.41">
  <actions/>
  <description>A sample pipeline job created via API</description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <pipelineTriggers>
      <triggers class="vector">
        <jenkins.triggers.SCMTrigger>
          <spec>* * * * *</spec>
          <ignorePostCommitHooks>false</ignorePostCommitHooks>
        </jenkins.triggers.SCMTrigger>
      </triggers>
    </pipelineTriggers>
  </properties>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsScmFlowDefinition" plugin="workflow-cps@2.89">
    <scm class="jenkins.plugins.git.GitSCM" plugin="git@4.12.0">
      <configVersion>2</configVersion>
      <userRemoteConfigs>
        <jenkins.plugins.git.UserRemoteConfig>
          <url>https://github.com/omprakashbhanarkar19/jenkins.git</url>
          <credentialsId>your-credentials-id</credentialsId>
        </jenkins.plugins.git.UserRemoteConfig>
      </userRemoteConfigs>
      <branches class="list">
        <jenkins.plugins.git.BranchSpec>
          <name>*/main</name>
        </jenkins.plugins.git.BranchSpec>
      </branches>
      <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
      <extensions/>
    </scm>
    <scriptPath>Jenkinsfile</scriptPath>
  </definition>
  <triggers/>
</flow-definition>
